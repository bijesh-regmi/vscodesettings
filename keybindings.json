// Place your key bindings in this file to override the defaults
[
  {
    "key": "ctrl+shift+alt+m",
    "command": "workbench.action.toggleMenuBar"
  },
  {
    "key": "ctrl+alt+t",
    "command": "workbench.action.focusActivityBar"
  },
  {
    "key": "ctrl+alt+y",
    "command": "workbench.action.activityBarLocation.hide"
  },
  {
    "key": "ctrl+alt+oem_5",
    "command": "workbench.action.terminal.killAll",
    "when": "terminalHasBeenCreated || terminalIsOpen || terminalProcessSupported"
  },
  {
    "key": "ctrl+c t",
    "command": "workbench.action.terminal.clear",
    "when": "terminalHasBeenCreated || terminalProcessSupported"
  },
  {
    "key": "ctrl+insert",
    "command": "-editor.action.clipboardCopyAction"
  },
  {
    "key": "ctrl+c",
    "command": "-editor.action.clipboardCopyAction"
  },
  {
    "key": "ctrl+c",
    "command": "editor.action.clipboardCopyAction"
  },
  {
    "key": "ctrl+a",
    "command": "-editor.action.selectAll"
  },
  {
    "key": "ctrl+a",
    "command": "editor.action.selectAll"
  },
  {
    "key": "ctrl+n",
    "command": "-workbench.action.files.newUntitledFile"
  },
  {
    "key": "ctrl+n",
    "command": "explorer.newFile"
  },
  {
    "key": "shift+alt+s shift+alt+right",
    "command": "workbench.action.splitEditorRight"
  },
  {
    "key": "ctrl+k ctrl+oem_5",
    "command": "-workbench.action.splitEditorRight"
  },
  {
    "key": "ctrl+shift+n",
    "command": "explorer.newFolder"
  },
  {
    "key": "ctrl+shift+alt+s",
    "command": "workbench.action.toggleStatusbarVisibility"
  },
  {
    "key": "ctrl+alt+left",
    "command": "-workbench.action.moveEditorToPreviousGroup"
  },
  {
    "key": "ctrl+alt+left",
    "command": "editor.action.moveCarretLeftAction"
  },
  {
    "key": "ctrl+alt+right",
    "command": "-workbench.action.moveEditorToNextGroup"
  },
  {
    "key": "ctrl+alt+right",
    "command": "editor.action.moveCarretRightAction"
  },
  {
    "key": "ctrl+alt+enter",
    "command": "editor.action.insertLineAfter",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+i",
    "command": "workbench.panel.chat",
    "when": "workbench.panel.chat.view.copilot.active"
  },
  {
    "key": "ctrl+alt+i",
    "command": "-workbench.panel.chat",
    "when": "workbench.panel.chat.view.copilot.active"
  }
]